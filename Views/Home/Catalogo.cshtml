@model List<MyMClothing.Models.ObViewModels.CategoriaViewModel>
@{
    ViewBag.Title = "Categorias";
}

<div class="container">
    <div class="row mt-5">
        @foreach (var cat in Model)
        {
            <div class="col-12 col-sm-6 col-md-4 col-lg-3 mb-3">
                <a href="/home/Categoria/@cat.id">
                    <img src="~/img/@cat.url_imagen" class="img-fluid" alt="...">
                </a>
                
            </div>
        }

    </div>
</div>

